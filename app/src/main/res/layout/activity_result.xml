<?xml version="1.0" encoding="utf-8"?>
<RelativeLayout xmlns:android="http://schemas.android.com/apk/res/android"
    xmlns:tools="http://schemas.android.com/tools"
    android:layout_width="match_parent"
    android:layout_height="match_parent"
    tools:context="si3901.rinafdlh.gustarinanurfadilah_1202154313_modul1.MainActivity">

    <TextView
        android:id="@+id/makanMalam"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:layout_alignParentTop="true"
        android:layout_centerHorizontal="true"
        android:layout_marginTop="38dp"
        android:text="Makan Malam"
        android:textSize="21sp" />

    <TextView
        android:id="@+id/Tempat"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:layout_below="@+id/makanMalam"
        android:layout_centerHorizontal="true"
        android:text="dengan Milea" />

    <TextView
        android:id="@+id/textMenu"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:layout_below="@+id/Tempat"
        android:layout_centerHorizontal="true"
        android:layout_marginTop="31dp"
        android:text="Menu" />

    <TextView
        android:id="@+id/Menu"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:layout_below="@+id/textMenu"
        android:layout_centerHorizontal="true"
        android:layout_marginTop="31dp"
        android:text="TextView"
        android:textSize="20sp" />

    <TextView
        android:id="@+id/textPorsi"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:layout_below="@+id/Menu"
        android:layout_centerHorizontal="true"
        android:layout_marginTop="33dp"
        android:text="Porsi" />

    <TextView
        android:id="@+id/Porsi"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:layout_below="@+id/textPorsi"
        android:layout_centerHorizontal="true"
        android:layout_marginTop="23dp"
        android:text="TextView"
        android:textSize="20sp" />

    <TextView
        android:id="@+id/textHarga"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:layout_below="@+id/Porsi"
        android:layout_centerHorizontal="true"
        android:layout_marginTop="30dp"
        android:text="Harga" />

    <TextView
        android:id="@+id/Harga"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:layout_below="@+id/textHarga"
        android:layout_centerHorizontal="true"
        android:layout_marginTop="40dp"
        android:textSize="20sp" />

</RelativeLayout>
